{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MoVibe",
  "version": "0.1.1",
  "identifier": "xyz.smbcloud.Movibe",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MoVibe",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "minimumSystemVersion": "13.0",
      "developmentTeam": "NADBSNEZWH"
    },
    "android": {
      "minSdkVersion": 24
    }
  }
}
